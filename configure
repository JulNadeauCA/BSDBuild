		if [ "$?" = "0" ]; then
			HAVE_MANDOC="yes"
			MANDOC="${path}/mandoc"
			break;
		fi
	elif [ -x "${path}/nroff" ]; then
		cat conftest.1 | ${path}/nroff -Tmandoc >/dev/null
		if [ "$?" = "0" ]; then
			HAVE_MANDOC="yes"
